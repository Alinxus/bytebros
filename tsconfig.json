{
  "compilerOptions": {
    "target": "ESNext",
    "module": "NodeNext",
    "strict": false,
    "skipLibCheck": true,
    "types": [
      "node"
    ],
    "jsx": "react-jsx",
    "jsxImportSource": "hono/jsx",
    "outDir": "./dist",
    "noImplicitAny": false
  },
  "exclude": ["node_modules", "frontend", "dist/frontend"]
}
